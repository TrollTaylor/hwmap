<item behavior="key" minimap="menus/minimap.xml:bronzekey">
	<behavior>
		<dictionary>
			<entry name="pickup-text"><string>item.keybronze</string></entry>					
			<entry name="type"><int>0</int></entry>
			<entry name="pickup-snd"><string>sound/misc.xml:take_key</string></entry>
			<entry name="pickup-particle"><string>effects/particles.xml:none</string></entry>
		</dictionary>
	</behavior>

	<sprite scale="16">
		<texture>items/items.png</texture>
		<origin>8 13</origin>
		<frame time="200">0 0 16 16</frame>	
		<frame time="200">16 0 16 16</frame>	
		<frame time="200">32 0 16 16</frame>	
		<frame time="200">16 0 16 16</frame>	
		<glow scale="16">
			<texture>items/items.png</texture>
				<frame time="200">48 0 16 16</frame>	
				<frame time="200">64 0 16 16</frame>	
				<frame time="200">80 0 16 16</frame>	
				<frame time="200">64 0 16 16</frame>	
		</glow>									
	</sprite>

	<collision shoot-through="true">
		<polygon>
			<point>-4 3</point>
			<point>-4 -7</point>
			<point>4 -7</point>
			<point>4 3</point>
		</polygon>
	</collision>
</item>